{
    "name": "escarter/laravel-obfuscator",
    "description": "A powerful Laravel package for code obfuscation with encryption and variable name randomization",
    "type": "library",
    "keywords": ["laravel", "obfuscation", "encryption", "security", "code-protection"],
    "license": "MIT",
    "authors": [
        {
            "name": "Escarter",
            "email": "mbutuhescarter@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "nikic/php-parser": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Escarter\\LaravelObfuscator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Escarter\\LaravelObfuscator\\ObfuscatorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

